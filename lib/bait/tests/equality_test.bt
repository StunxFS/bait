fun test_array_equality() {
	a := [1, 2, 3]
	assert a == [1, 2, 3]

	mut b := [1, 2]
	b.push(3)
	assert a == b

	b.push_many([3, 2, 1])
	rb := b.reverse()
	c := rb.slice(0, 3)
	assert a == c
}

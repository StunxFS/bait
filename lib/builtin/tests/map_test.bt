fun test_type_init() {
	mut m := map[string]string
	m['a'] = 'A'
	m['b'] = 'B'
	assert m.length == 2
	assert m['b'] == 'B'
}

fun test_literal_init() {
	m := map{
		'a': 'A'
		'b': 'B'
	}
	assert m.length == 2
	assert m['b'] == 'B'
}

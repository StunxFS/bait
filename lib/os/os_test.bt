import os

fun test_dir(){
	l := '/foo/bar/file'
	assert os.dir(l) == '/foo/bar'

	w := 'C:\\foo\\bar\\file'
	assert os.dir(w) == 'C:\\foo\\bar'
}

fun test_read_lines() {
	lines := os.read_lines("${$BAITDIR}/lib/os/tests/3_lines.txt")
	assert lines.length == 3
	assert lines[1] == '2'
}
